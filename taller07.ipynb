    "# Ejercicio 02: contar verbos\n",
    "def contar_verbos(palabras: Pila[str]) -> int:\n",
    "    cont: int = 0\n",
    "    copia = palabras.copiar()\n",
    "    while not copia.vacia:\n",
    "        palabra = copia.tope\n",
    "        if palabra[-2:] in [\"ar\", \"er\", \"ir\"]:\n",
    "            cont += 1\n",
    "        copia.desapilar()\n",
    "    return cont"
