   "source": [
    "# Ejercicio 04\n",
    "def suma_pares_tres_digitos(pila: Pila[int]) -> int:\n",
    "    suma: int = 0\n",
    "    copia: Pila[int] = pila.copiar()\n",
    "    while not copia.vacia:\n",
    "        num = copia.tope\n",
    "        if 100 <= num <= 999 and num % 2 == 0:\n",
    "            suma += num\n",
    "        copia.desapilar()\n",
    "    return suma"
   ]
